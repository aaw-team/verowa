{
    "name": "aaw-team/verowa",
    "type": "typo3-cms-extension",
    "description": "Verowa Agenda integration for TYPO3",
    "keywords": ["verowa", "api","typo3"],
    "homepage": "https://github.com/aaw-team/verowa",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Agentur am Wasser Team",
            "email": "development@agenturamwasser.ch",
            "homepage": "https://web.agenturamwasser.ch/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "typo3/cms-core": "^10.4.20"
    },
    "require-dev": {
        "nimut/testing-framework": "^5.0"
    },
    "replace": {
        "typo3-ter/verowa": "self.version"
    },
    "autoload": {
        "psr-4": {
            "AawTeam\\Verowa\\": "Classes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AawTeam\\Verowa\\Tests\\": "Tests/"
        }
    },
    "support": {
        "source": "https://github.com/aaw-team/verowa",
        "issues": "https://github.com/aaw-team/verowa/issues"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "typo3/cms": {
            "app-dir": ".Build",
            "web-dir": ".Build/public",
            "extension-key": "verowa"
        }
    }
}
